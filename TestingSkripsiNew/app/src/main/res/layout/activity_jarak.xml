<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#2f2e2e"
    android:orientation="vertical"
    tools:context=".Jarak">

    <ImageButton
        android:id="@+id/button_back"
        android:layout_width="50dp"
        android:layout_height="45dp"
        android:background="#2F2E2E"
        android:src="@drawable/left_arrow"
        tools:layout_editor_absoluteX="10dp"
        tools:layout_editor_absoluteY="29dp"
        tools:ignore="MissingConstraints" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Posisi anda di kampus.."
        android:textColor="#4CCEC6"
        android:textSize="22dp"
        tools:layout_editor_absoluteX="88dp"
        tools:layout_editor_absoluteY="52dp"
        tools:ignore="MissingConstraints" />

    <TextView
        android:id="@+id/txt_lat"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Latittude   :"
        android:textColor="#ffffff"
        android:textSize="17dp"
        tools:layout_editor_absoluteX="64dp"
        tools:layout_editor_absoluteY="123dp"
        tools:ignore="MissingConstraints" />

    <TextView
        android:id="@+id/txt_statuslat"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="-"
        android:textColor="#ffffff"
        android:textSize="17dp"
        tools:ignore="MissingConstraints"
        tools:layout_editor_absoluteX="212dp"
        tools:layout_editor_absoluteY="122dp" />

    <TextView
        android:id="@+id/txt_long"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Longitude :"
        android:textColor="#ffffff"
        android:textSize="17dp"
        tools:ignore="MissingConstraints"
        tools:layout_editor_absoluteX="64dp"
        tools:layout_editor_absoluteY="160dp" />

    <TextView
        android:id="@+id/txt_statuslong"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="-"
        android:textColor="#ffffff"
        android:textSize="17dp"
        tools:ignore="MissingConstraints"
        tools:layout_editor_absoluteX="212dp"
        tools:layout_editor_absoluteY="160dp" />

    <TextView
        android:id="@+id/txt_jarak"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Posisi        :"
        android:textColor="#ffffff"
        android:textSize="17dp"
        tools:layout_editor_absoluteX="64dp"
        tools:layout_editor_absoluteY="197dp"
        tools:ignore="MissingConstraints" />

    <TextView
        android:id="@+id/txt_statusjarak"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="-"
        android:textColor="#ffffff"
        android:textSize="17dp"
        tools:ignore="MissingConstraints"
        tools:layout_editor_absoluteX="212dp"
        tools:layout_editor_absoluteY="197dp" />

    <Button
        android:id="@+id/btn_checkin"
        android:layout_width="120dp"
        android:layout_height="wrap_content"
        android:background="@drawable/custom_buttom3"
        tools:layout_editor_absoluteX="60dp"
        tools:layout_editor_absoluteY="263dp"
        tools:ignore="MissingConstraints"
        android:text="Check-in"
        android:textColor="#ffffff"/>

    <Button
        android:id="@+id/btn_checkout"
        android:layout_width="120dp"
        android:layout_height="wrap_content"
        android:background="@drawable/custom_buttom3"
        android:text="Check-out"
        android:textColor="#ffffff"
        tools:ignore="MissingConstraints"
        tools:layout_editor_absoluteX="217dp"
        tools:layout_editor_absoluteY="263dp" />


</androidx.constraintlayout.widget.ConstraintLayout>